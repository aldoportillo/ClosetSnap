<%= form_with url: insert_itemcategory_path, method: :post do |form| %>
  <td>
    <%= form.collection_select :category_id, Category.all, :id, :name, include_blank: false %>
  </td>
  <%= form.number_field :item_id, value: @item.id, hidden: true %>
  <td>
    <%= form.submit 'Add Category' %>
  </td>
<% end %>
