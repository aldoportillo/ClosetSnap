<%= form_with url: insert_itemoutfit_path, method: :post do |form| %>
  <%= form.label :item_id, 'Select an Item to Add' %>
  <%= form.collection_select :item_id, Item.where(:user_id => current_user.id), :id, :name, include_blank: false %>
  <div>
    <%= form.number_field :outfit_id, value: @outfit.id, hidden: true %>
  </div>

  <%= form.submit 'Add Item' %>
  <% end %>
