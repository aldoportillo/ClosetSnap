<div id="<%= dom_id item %>">
  <p>
    <strong>Name:</strong>
    <%= item.name %>
  </p>

  <p>
    <strong>Image</strong>
    <img src="<%= item.image_url %>" alt="Image">
  </p>

  <p>
    <strong>Size:</strong>
    <%= item.size %>
  </p>

  <p>
    <strong>Compliments:</strong>
    <%= item.compliments %>
  </p>

  <p>
    <strong>User:</strong>
    <%= item.user_id %>
  </p>

</div>
